{
  "GOVUK / components / date / ": {
		"prefix": "gov-njk-date",
		"body": [
			"{% from \"date-input/macro.njk\" import govukDateInput %}",
			"",
			"{{ govukDateInput({",
			"  id: \"dob\",",
			"  name: \"dob\",",
			"  fieldset: {",
			"    legend: {",
			"      text: \"What is your date of birth?\",",
			"      isPageHeading: true,",
			"      classes: \"govuk-fieldset__legend--xl\"",
			"    }",
			"  },",
			"  hint: {",
			"    text: \"For example, 31 3 1980\"",
			"  },",
			"  items: [",
			"    {",
			"      name: \"day\"",
			"    },",
		  "    {",
			"      name: \"month\"",
			"    },",
			"    {",
			"      name: \"year\"",
			"    }",
			"  ]",
			"}) }}"
		],
		"description": "GOVUK: Nunjucks date component"
	},
	"GOVUK / components / date (errors) / ": {
		"prefix": "gov-njk-date-with-errors",
		"body": [
			"{% from \"date-input/macro.njk\" import govukDateInput %}",
			"",
			"{{ govukDateInput({",
			"  id: \"dob\",",
			"  name: \"dob\",",
			"  fieldset: {",
			"    legend: {",
			"      text: \"What is your date of birth?\",",
			"      isPageHeading: true,",
			"      classes: \"govuk-fieldset__legend--xl\"",
			"    }",
			"  },",
			"  hint: {",
			"    text: \"For example, 31 3 1980\"",
			"  },",
			"  errorMessage: {",
			"    text: \"Date of birth must be in the past\"",
			"  },",
			"  items: [",
			"    {",
			"      name: \"day\",",
			"      value: \"6\"",
			"    },",
			"    {",
			"      name: \"month\",",
			"      value: \"3\"",
			"    },",
			"    {",
			"      name: \"year\",",
			"      value: \"2076\"",
			"    }",
			"  ]",
			"}) }}"
		],
		"description": "GOVUK: Nunjucks date component with errors"
  }
