	"GOVUK input date": {
		"prefix": "govuk-input-date",
		"body": [
			"<div class=\"form-group\">",
			"  <fieldset>",
			"    <legend>",
			"      <span class=\"form-label-bold\">",
			"        What is your date of birth?",
